{
  "_comment": "Language configurations for code execution. Time/memory multipliers adjust limits based on language performance characteristics.",
  "cpp": {
    "name": "C++",
    "extension": ".cpp",
    "compileCmd": "g++ -O2 -std=c++17 -o solution.exe {source}",
    "executeCmd": "solution.exe",
    "compileTimeout": 30000,
    "timeMultiplier": 1.0,
    "supportsCompilation": true,
    "pathAdditions": [
      "/usr/bin",
      "/usr/local/bin",
      "C:\\msys64\\mingw64\\bin"
    ],
    "mainFile": "solution.cpp",
    "outputFile": "solution.exe",
    "errorPatterns": [
      "error:",
      "undefined reference",
      "compilation terminated"
    ]
  },
  "java": {
    "name": "Java",
    "extension": ".java",
    "compileCmd": "javac {source}",
    "executeCmd": "java Solution",
    "compileTimeout": 30000,
    "timeMultiplier": 2.0,
    "supportsCompilation": true,
    "pathAdditions": [
      "$JAVA_HOME/bin",
      "/usr/lib/jvm/java-21-openjdk-amd64/bin",
      "/usr/lib/jvm/java-17-openjdk-amd64/bin",
      "/usr/lib/jvm/java-11-openjdk-amd64/bin",
      "/usr/lib/jvm/default-java/bin"
    ],
    "mainFile": "Solution.java",
    "outputFile": "Solution.class",
    "requiredClass": "Solution",
    "errorPatterns": [
      "error:",
      "cannot find symbol",
      "class not found"
    ]
  },
  "python": {
    "name": "Python",
    "extension": ".py",
    "executeCmd": "python3 {source}",
    "compileTimeout": 0,
    "timeMultiplier": 3.0,
    "supportsCompilation": false,
    "pathAdditions": [
      "/usr/bin",
      "/usr/local/bin",
      "$PYENV_ROOT/shims",
      "$HOME/.pyenv/shims",
      "$HOME/.asdf/shims"
    ],
    "mainFile": "solution.py",
    "errorPatterns": [
      "SyntaxError:",
      "IndentationError:",
      "NameError:",
      "TypeError:"
    ]
  }
}
