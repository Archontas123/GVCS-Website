{
  "name": "programming-contest-platform",
  "version": "1.0.0",
  "description": "School Programming Contest Platform with ICPC-style scoring",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "echo 'Backend build complete - no compilation needed for Node.js'",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "db:test": "node src/tests/test-db-connection.js",
    "db:setup": "./setup-database.sh",
    "db:migrate": "knex migrate:latest",
    "db:migrate:rollback": "knex migrate:rollback",
    "db:migrate:status": "knex migrate:status",
    "db:migrate:make": "knex migrate:make",
    "db:seed": "knex seed:run",
    "db:reset": "knex migrate:rollback --all && knex migrate:latest && knex seed:run",
    "db:fresh": "npm run db:reset",
    "db:validate": "node src/tests/validate-schema.js",
    "judge:build": "./scripts/build-judge.sh",
    "judge:test": "docker-compose -f docker-compose.execution.yml up judge-test",
    "test:execution": "cross-env NODE_ENV=test jest --testNamePattern=\"execution\"",
    "validate:phase1.3": "node src/tests/validate-phase-1.3.js",
    "setup:complete": "npm run db:setup && npm run db:migrate && npm run db:seed && npm run judge:build"
  },
  "keywords": [
    "competitive-programming",
    "icpc-scoring",
    "contest-platform",
    "algorithm",
    "coding-contest"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.2",
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.0.1",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.1",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.8.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "cross-env": "^10.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
