

NODE_ENV=production
PORT=3000

# Frontend URL for CORS
FRONTEND_URL=https://your-frontend-domain.com

# Security - These should be overridden with actual secrets
JWT_SECRET=REPLACE_WITH_ACTUAL_SECRET
JWT_EXPIRES_IN=24h

# Database - PostgreSQL
DB_HOST=postgres
DB_PORT=5432
DB_NAME=hackathon_db
DB_USER=hackathon_user
DB_PASSWORD=REPLACE_WITH_ACTUAL_PASSWORD
DATABASE_URL=postgresql://hackathon_user:REPLACE_WITH_ACTUAL_PASSWORD@postgres:5432/hackathon_db

# Test Database (for production testing)
TEST_DATABASE_URL=postgresql://hackathon_user:REPLACE_WITH_ACTUAL_PASSWORD@postgres:5432/hackathon_test_db
TEST_DB_HOST=postgres
TEST_DB_PORT=5432
TEST_DB_NAME=hackathon_test_db
TEST_DB_USER=hackathon_user
TEST_DB_PASSWORD=REPLACE_WITH_ACTUAL_PASSWORD

# Redis
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=REPLACE_WITH_ACTUAL_PASSWORD
REDIS_DB=0

# Judge System
JUDGE_WORKERS=4

# Session Management
LOG_LEVEL=warn
SESSION_TIMEOUT_MINUTES=120

# Admin
ADMIN_TOKEN_EXPIRES=8h