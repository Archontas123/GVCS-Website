
NODE_ENV=development
PORT=3001

# Frontend URL for CORS
FRONTEND_URL=http://localhost:3000

# Security - Development keys (not for production)
JWT_SECRET=development-jwt-secret-key-not-for-production
JWT_EXPIRES_IN=24h

# Database - PostgreSQL
DB_HOST=postgres
DB_PORT=5432
DB_NAME=hackathon_db
DB_USER=hackathon_user
DB_PASSWORD=hackathon_password
DATABASE_URL=postgresql://hackathon_user:hackathon_password@postgres:5432/hackathon_db

# Test Database
TEST_DATABASE_URL=postgresql://hackathon_user:hackathon_password@postgres:5432/hackathon_test_db
TEST_DB_HOST=postgres
TEST_DB_PORT=5432
TEST_DB_NAME=hackathon_test_db
TEST_DB_USER=hackathon_user
TEST_DB_PASSWORD=hackathon_password

# Redis
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# Judge System
JUDGE_WORKERS=2

# Session Management
LOG_LEVEL=debug
SESSION_TIMEOUT_MINUTES=120

# Admin
ADMIN_TOKEN_EXPIRES=8h