{
  "name": "cs-club-programming-contest-platform",
  "version": "1.0.0",
  "description": "GVHS CS Club Programming Contest Platform - ICPC-style contest management system",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm start",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "start": "start.bat",
    "start:unix": "./start.sh",
    "start:npm": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd backend && NODE_ENV=development npm start",
    "start:frontend": "cd frontend && GENERATE_SOURCEMAP=false npm start",
    "stop": "stop.bat",
    "stop:unix": "./stop.sh",
    "test": "npm run test:backend && npm run test:frontend && npm run test:e2e",
    "test:backend": "cd backend && npm test",
    "test:frontend": "cd frontend && npm test -- --watchAll=false",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:auth": "playwright test tests/e2e/specs/auth",
    "test:e2e:contest": "playwright test tests/e2e/specs/contest",
    "test:e2e:submissions": "playwright test tests/e2e/specs/submissions",
    "test:e2e:leaderboard": "playwright test tests/e2e/specs/leaderboard",
    "test:e2e:admin": "playwright test tests/e2e/specs/admin",
    "test:e2e:chrome": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:multi": "playwright test tests/e2e/specs/multi-browser",
    "install:all": "npm install && npm run install:backend && npm run install:frontend",
    "install:backend": "cd backend && npm install",
    "install:frontend": "cd frontend && npm install",
    "db:migrate": "cd backend && npm run db:migrate",
    "db:rollback": "cd backend && npm run db:rollback",
    "db:seed": "cd backend && npm run db:seed",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "lint": "npm run lint:backend && npm run lint:frontend",
    "lint:backend": "cd backend && npm run lint",
    "lint:frontend": "cd frontend && npm run lint",
    "clean": "npm run clean:backend && npm run clean:frontend",
    "clean:backend": "cd backend && rm -rf node_modules build",
    "clean:frontend": "cd frontend && rm -rf node_modules build"
  },
  "workspaces": [
    "frontend",
    "backend"
  ],
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/node": "^24.4.0",
    "concurrently": "^7.6.0",
    "typescript": "^5.9.2"
  },
  "overrides": {
    "schema-utils@2.7.0": {
      "ajv": "6.12.6",
      "ajv-keywords": "3.5.2"
    },
    "schema-utils@2.7.1": {
      "ajv": "6.12.6",
      "ajv-keywords": "3.5.2"
    }
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/cs-club-programming-contest-platform.git"
  },
  "keywords": [
    "programming-contest",
    "contest",
    "icpc",
    "programming",
    "competition",
    "education"
  ],
  "author": "GVHS CS Club",
  "license": "MIT"
}
